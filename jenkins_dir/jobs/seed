url=""
binding.variables.each {
   if ( "${it.key}" == "URL" ) {
      url="${it.value}"
   }
}

pipelineJob('Pipeline') {
    description 'Build and test the app.'
    scm {
        github 'tjordanchat/jenkins_setup'
    }
  }
  definition {
    cps {
      sandbox()
      script("def URL = '"+url+"'") 
    }
  }
}
